{
  "name": "express-4x-api-starter",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "start": "cross-env NODE_ENV=production pm2 ./src/index.js",
    "start:docker": "docker run -d -p 3301:3301 express-4x-api-starter",
    "start:dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "lint": "eslint -c .eslintrc ./src",
    "lint:fix": "eslint -c .eslintrc ./src --fix",
    "prettier": "prettier --check \"src/**/*.js\"",
    "prettier:write": "prettier --check \"src/**/*.js\" --write",
    "build:docker": "docker build . -t express-4x-api-starter",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.2"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
